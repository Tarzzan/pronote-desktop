{
  "project": "pronote-desktop",
  "current_version": "1.4.0",
  "next_version": "1.5.0",
  "architecture": "amd64",
  "maintainer": "Tarzzan <tarzzan@users.noreply.github.com>",
  "homepage": "https://github.com/Tarzzan/pronote-desktop",
  "install_dir": "/usr/lib/pronote-desktop",
  "venv_dir": "/usr/lib/pronote-desktop/python-env",
  "wheels_dir": "/usr/lib/pronote-desktop/wheels",
  "config_dir": "/etc/pronote-desktop",
  "systemd_service": "pronote-desktop-api.service",
  "api_port": 5174,
  "python_wheels": [
    "autoslot",
    "beautifulsoup4",
    "blinker",
    "certifi",
    "charset-normalizer",
    "click",
    "flask",
    "flask-cors",
    "idna",
    "itsdangerous",
    "jinja2",
    "markupsafe",
    "pronotepy",
    "pycryptodome",
    "requests",
    "soupsieve",
    "typing-extensions",
    "urllib3",
    "werkzeug"
  ],
  "wheel_versions": {
    "pronotepy": "2.14.5",
    "flask": "3.1.3",
    "flask-cors": "6.0.2",
    "pycryptodome": "3.23.0",
    "werkzeug": "3.1.6",
    "requests": "2.32.5",
    "jinja2": "3.1.6",
    "click": "8.3.1",
    "urllib3": "2.6.3",
    "certifi": "2026.1.4",
    "charset-normalizer": "3.4.4",
    "idna": "3.11",
    "beautifulsoup4": "4.14.3",
    "blinker": "1.9.0",
    "itsdangerous": "2.2.0",
    "markupsafe": "3.0.3",
    "soupsieve": "2.8.3",
    "typing-extensions": "4.15.0",
    "autoslot": "2025.11.1"
  },
  "deb_structure": {
    "app_files": "usr/lib/pronote-desktop/",
    "launcher": "usr/bin/pronote-desktop",
    "desktop_entry": "usr/share/applications/pronote-desktop.desktop",
    "icons": "usr/share/icons/hicolor/{size}x{size}/apps/pronote-desktop.png",
    "icon_sizes": [16, 32, 48, 128, 256, 512],
    "metainfo": "usr/share/metainfo/fr.pronote.desktop.metainfo.xml",
    "systemd": "etc/systemd/system/pronote-desktop-api.service",
    "config": "etc/pronote-desktop/config.json"
  },
  "build_command": "dpkg-deb --build /tmp/pronote-v{VERSION} pronote-desktop_{VERSION}_offline_amd64.deb",
  "pip_download_command": "python3 -m pip download pronotepy flask flask-cors --dest /tmp/wheels",
  "notes": "Toujours tester le paquet sur Ubuntu 22.04 et 24.04 avant de publier la release."
}
